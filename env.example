# LLEMA Environment Configuration
# Copy this file to .env and fill in your actual API keys

# DeepInfra API Key for LLM calls
OPENAI_API_KEY=your_openai_api_key_here

# Materials Project API Key for property extraction
MATERIALS_PROJECT_API_KEY=your_materials_project_api_key_here

# Surrogate models directory (optional, defaults to src/surrogate_models)
SURROGATE_MODELS_DIR=src/surrogate_models

# Debug settings (optional)
EVOLUTION_DEBUG_PRINT_PROMPT=1
